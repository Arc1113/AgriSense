{
  "metadata": {
    "version": "1.0",
    "description": "Gold-standard evaluation dataset for AgriSense RAG pipeline. 100 test cases (10 per disease class + 10 for Healthy). Each case has a user query, the correct disease, expected keywords that MUST appear in a good answer, and hallucination keywords that must NOT appear.",
    "diseases": [
      "Bacterial Spot",
      "Early Blight",
      "Late Blight",
      "Leaf Mold",
      "Septoria Leaf Spot",
      "Spider Mites",
      "Target Spot",
      "Yellow Leaf Curl Virus",
      "Mosaic Virus",
      "Healthy"
    ],
    "total_cases": 100,
    "metrics": [
      "grounded_response_rate",
      "citation_precision",
      "hallucination_rate",
      "latency_p50_p95",
      "ablation_reranker",
      "ablation_rag_vs_llm_only"
    ]
  },
