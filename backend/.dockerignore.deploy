.vscode
.idea
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Testing
.pytest_cache
.coverage
htmlcov/

# Logs
*.log
server_log.txt

# Git
.git
.gitignore

# Jupyter
.ipynb_checkpoints

# Distribution
dist/
build/
*.egg-info/

